#! /usr/bin/env bash
#$ -cwd            ## preserve working directory
#$ -j yes          ## merge stdout and stderr

## Usage:
## ./rscript.sge
## qsub rscript.sge

echo "Running $0 ..."

## Required Linux environment modules
module load CBI r

Rscript rscript.R

## End-of-job summary
